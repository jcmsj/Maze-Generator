    "version": "0.2.0",
    "configurations": [

        {
            "name": "Provides a maze file",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-f", "maze.json"],
            "justMyCode": true
        },
        {
            "name": "Maze generation",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-l", "5", "-w", "5", "-export", "maze.json"],
            "justMyCode": true
        },
        {
            "name": "random DLS",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-l", "5", "-w", "5", "-export", "maze.json", "-limit","5"],
            "justMyCode": true
        }
    ]
}
